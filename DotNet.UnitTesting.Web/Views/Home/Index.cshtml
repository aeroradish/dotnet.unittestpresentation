@{
    ViewBag.Title = "Macon .NET User Group";
    ViewBag.Message = "Presented by Kyle Kelly";
}
@section featured {
    <section class="featured">
        <div class="content-wrapper">
            <hgroup class="title">
                <h1>@ViewBag.Title.</h1>
                <h2>@ViewBag.Message</h2>
            </hgroup>
            <p>
                Some code was adapted from the following source, so to learn more about Unit Testing visit
                <a href="https://www.manning.com/books/the-art-of-unit-testing-second-edition" title="Art of Unit Testing">The Art of Unit Testing, Second Edition</a>
                    by Roy Osherove with examples in C# .
            </p>
            <p>
                Code for this project can be found here
                <a href="https://bitbucket.org/kyleskelly/dotnet.unittestpresentation" title="Code" target="_blank">https://bitbucket.org/kyleskelly/dotnet.unittestpresentation</a>
                
            </p>
        </div>
    </section>
}
